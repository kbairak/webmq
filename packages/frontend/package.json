{
  "name": "webmq-frontend",
  "version": "1.0.0",
  "description": "Frontend library for WebMQ",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --emitDeclarationOnly && esbuild src/index.ts --bundle --platform=browser --outfile=dist/index.js --format=esm && esbuild src/index.ts --bundle --platform=node --outfile=dist/index.cjs --format=cjs",
    "dev": "esbuild src/index.ts --bundle --platform=browser --outfile=dist/index.js --format=esm --watch",
    "test": "jest",
    "clean": "rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/uuid": "^10.0.0",
    "esbuild": "^0.25.9",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "events": "^3.3.0",
    "uuid": "^10.0.0"
  }
}
