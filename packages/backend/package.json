{
  "name": "webmq-backend",
  "version": "1.0.0",
  "description": "Backend library for WebMQ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "mkdir -p src/common && cp ../../common/logger.ts src/common/ && echo '// Auto-generated from /common - do not edit directly' | cat - src/common/logger.ts > temp && mv temp src/common/logger.ts",
    "build": "npm run prebuild && tsc",
    "dev": "npm run prebuild && tsc -w",
    "test": "npm run prebuild && jest",
    "clean": "rm -rf dist src/common"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.3",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/ws": "^8.5.5",
    "typescript": "^5.1.6",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.3"
  }
}
